<hibernate-mapping package="com.github.conradovera.xmlmapping">
  <class name="LugarRamses" table="LUGAR">
    <id column="LUGAR_ID" name="id">
      <generator class="increment"/>
    </id>
    <property column="LUGAR_NOMBRE" name="nombre" type="string"/>
    <property column="LUGAR_CIUDAD" name="ciudad" type="string"/>
  </class>
</hibernate-mapping>
