<hibernate-mapping package="com.github.conradovera.xmlmapping">
  <class name="EventRamses" table="EVENTS">
    <id column="EVENT_ID" name="id">
      <generator class="increment"/>
    </id>
    <property column="EVENT_IdLUGAR" name="idLugar" type="string"/>
    <property column="EVENT_DATE" name="date" type="timestamp"/>
    <property column="EVENT_TITTLE" name="title" type="string"/>
  </class>
</hibernate-mapping>
